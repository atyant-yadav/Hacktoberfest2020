#include<iostream>
using namespace std;

int fib(int n)
{
  int f_term=0,s_term=1,sum,count=2;
  if(n==1)
  {
    return 0;
  }
  else if(n==1)
  {
    return 1;
  }
  else
  {
    while(count<=n)
    {
      sum=f_term+s_term;
      count++;
    }
    return sum;
  }
}
int main()
{
  int n,result;
  cout<<"Enter the value of n ";
  cin>>n;
  result=fib(n);
  cout<<"The fibonacci series number at "<<n<<" place is "<<result<<"\n;
}
